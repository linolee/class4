<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.sist.admin">

<resultMap type="kr.co.sist.admin.domain.MemberListDomain" id="MemberListDomain">
	<result column="client_id" property="client_id"/>
	<result column="name" property="name"/>
	<result column="birth" property="birth"/>
	<result column="gender" property="gender"/>
	<result column="email" property="email"/>
</resultMap>

<select id="clientTotalCnt" resultType="int">
	select count(*) cnt 
	from client
</select>

<select id="teacherInfo" resultType="java.lang.String">
	select status 
	from teacher 
	where status='Y' and client_id=#{id}
</select>



<select id="selectClient" resultMap="MemberListDomain">
select client_id, name, birth, gender, email
from client
</select>


</mapper>