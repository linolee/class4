<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.sist.user.mapper.login_mapper">
<resultMap type="kr.co.sist.user.domain.LoginDomain" id="loginResult">
	<result column="client_id" property="client_id"/>
	<result column="name" property="name"/>
	<result column="status" property="status"/>
</resultMap>
<select id="selectAccount" resultMap="loginResult" parameterType="kr.co.sist.user.vo.UserLoginVO">
	select client_id, name, status
	from client
	where client_id = #{id} and pass=#{pass} 
</select>

</mapper>









